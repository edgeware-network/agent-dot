{
  "name": "agent-dot",
  "version": "1.5.3",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "tsc": "tsc --noEmit",
    "build": "next build",
    "start": "next start",
    "lint": "cross-env NODE_ENV=development eslint .",
    "lint:prod": "cross-env NODE_ENV=production eslint .",
    "format": "prettier --check .",
    "format:fix": "prettier --write --list-different .",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "scripts:build": "tsc -p tsconfig.node.json",
    "path:fix": "pnpm scripts:build && node scripts/dist/fix-path.js",
    "path:revert": "pnpm scripts:build && node scripts/dist/revert-path.js",
    "postinstall": "pnpm path:fix && pnpm papi",
    "db:generate": "drizzle-kit generate --config=./drizzle.config.ts",
    "db:migrate": "tsx --env-file=.env.local db/migrate.ts",
    "db:drop": "drizzle-kit drop",
    "db:pull": "drizzle-kit introspect",
    "db:push": "drizzle-kit push",
    "commitlint": "commitlint --edit"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@neondatabase/serverless": "^1.0.1",
    "@polkadot-api/descriptors": "file:.papi/descriptors",
    "@polkadot/ui-shared": "^3.15.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@t3-oss/env-nextjs": "^0.13.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "detect-browser": "^5.3.0",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "nanoid": "^5.1.5",
    "next": "15.4.2",
    "pino": "^9.7.0",
    "polkadot-api": "^1.15.0",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-icons": "^5.5.0",
    "rxjs": "^7.8.2",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/format": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.4.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  }
}